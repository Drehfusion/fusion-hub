@charset "utf-8";

@import url("http://fonts.googleapis.com/css?family=Open+Sans:400,700");

html { background-color: rgb(234, 240, 242); }

body { font: 14px / 1.5 Arial, Helvetica, sans-serif; }

header { text-align: center; padding-top: 100px; margin-bottom: 200px; }

header h1 { font: 32px / 1.5 "Open Sans", sans-serif; color: rgb(63, 113, 174); padding-bottom: 16px; }

header h2 { color: rgb(240, 82, 131); }

header h2 a { color: inherit; text-decoration: none; display: inline-block; border: 1px solid rgb(240, 82, 131); padding: 10px 15px; border-radius: 3px; font: bold 14px / 1 "Open Sans", sans-serif; text-transform: uppercase; }

header h2 a:hover { background-color: rgb(240, 82, 131); transition: all 0.2s ease 0s; color: rgb(255, 255, 255); }

header ul { max-width: 600px; margin: 60px auto 0px; }

header ul a { text-decoration: none; color: rgb(255, 255, 255); text-align: left; background-color: rgb(185, 193, 202); padding: 10px 16px; border-radius: 2px; opacity: 0.8; font-size: 16px; display: inline-block; margin: 4px; line-height: 1; outline: none; transition: all 0.2s ease 0s; }

header ul li a.active { background-color: rgb(102, 182, 80); pointer-events: none; }

header ul li a:hover { opacity: 1; }

header ul { list-style: none; padding: 0px; }

header ul li { display: inline-block; }

footer { position: fixed; bottom: 0px; }

@media (max-height: 800px) {
  footer { position: static; }
  header { padding-top: 40px; }
}
